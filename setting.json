{
	"$schema": "https://aka.ms/terminal-profiles-schema",
	"alwaysShowTabs": true,
	"defaultProfile": "{00000000-0000-0000-0000-000000000000}",
	"initialCols": 120,
	"initialRows": 30,
	"keybindings": [
		{
			"command": "closeTab",
			"keys": [
				"ctrl+w"
			]
		},
		{
			"command": "newTab",
			"keys": [
				"ctrl+t"
			]
		}
	],
	"requestedTheme": "dark",
