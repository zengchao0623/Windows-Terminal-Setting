			"fontFace": "Consolas",
			"fontSize": 10,
			"guid": "{00000000-0000-0000-0000-000000000000}",
			"historySize": 9001,
			"icon": "ms-appx:///ProfileIcons/{00000000-0000-0000-0000-000000000000}.png",
			"name": "PowerShell",
			"tabTitle": "PowerShell",
			"padding": "0, 0, 0, 0",
			"snapOnInput": true,
			"startingDirectory": "%USERPROFILE%",
			"useAcrylic": true
